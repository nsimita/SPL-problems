//problem 3
#include <stdio.h>
int add(int n); // function prototype
int main() {
  int n;
  scanf("%d", &n); // how many numbers for sum
  int result = add(n); // calling function
  printf("Sum In Function: %d", result );
    return 0;
}
int add(int n) //n is the number of sum{
   int sum=0, i, num;
   for(i=1; i<=n; i++){
       scanf("%d",&num); // taking n inputs for sum
       sum= sum+num;
   }
   return sum; //it will return sum to the main
}
