#include <stdio.h>

int main() {
  int n, count=0;
  char a[100];
  scanf("%d",&n);
  for(int i=0; i<=n-1; i++){
      scanf(" %c",&a[i]);
  }
  for(int i=0; i<=n-1; i++){
      if(a[i]== 'a'||a[i]=='e'||a[i]== 'i'||a[i]== 'o'||a[i]== 'u'||a[i]== 'A'||a[i]== 'E'||a[i]== 'I'||a[i]== 'O'||a[i]== 'U'){
          count++;
      }
      
      
  }
      printf("%d",count);

    return 0;
}
