//problem 8  
#include <stdio.h>
int minVal(int n);
int main() {
  int n;
  scanf("%d",&n);
  printf("Minimum Value: %d",minVal(n));
    return 0;
}
int minVal(int n){
    int arr[n], i;
    int min=arr[0];
    for(i=0; i<n; i++){
        scanf("%d",&arr[i]);
    }
    for(i=0; i<n; i++){
        if (min>arr[i]){
         min=arr[i];
     }
    }
    return min;
}
