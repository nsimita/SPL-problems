//function problem 7
#include <stdio.h>
int arrEven(int n);
int main() {
  int n;
  scanf("%d",&n);
  arrEven(n);
    return 0;
}
int arrEven(int n){
    int arr[n], i;
    for(i=0; i<n; i++){
        scanf("%d",&arr[i]);
    }
    for(i=0; i<n; i++){
       if(arr[i]%2==0){
           printf("%d ",arr[i]);
       }
    }
    return arr[n];
}
