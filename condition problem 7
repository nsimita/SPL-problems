#include <stdio.h>
// prob 7
int main() {
   int a,b;
   
   scanf("%d%d",&a,&b);
   if (a>b){
       printf("%d is greater than %d",a,b);
       
   }
   else if (a==b){
       printf("%d is equal to %d",a,b);
       
   }
   else if (a<b){
       printf("%d is less than %d",a,b);
       
   }
   else {
       printf("invalid");
   }
    

    return 0;
}
