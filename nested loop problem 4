#include <stdio.h>
//nested loop 4
int main() {
    int n;
    scanf("%d",&n);
    for(int i=1; i<=n ; i++){
        for(int j =1; j<=n-i; j++){
            printf("_");
            
        }
         for(int k=1; k<=i; k++){
            printf("%d",i);
           
        }
      printf("\n"); 
    }
    return 0;
}
