#include <stdio.h>
//problem 9
int main() {
    int num[10000];
    int i,n;
    scanf("%d",&n);
    for(i=0; i<=n-1;i++){
        scanf("%d",&num[i]);
    }
    printf("Array A: ");
    for (i=0; i<=n-1; i++){
        printf("%d ",num[i]);
    }
    printf("\n");
    printf("Array B: ");
    for (i=n-1; i>=0; i--){
        printf("%d ",num[i]);
    }
    return 0;
}
