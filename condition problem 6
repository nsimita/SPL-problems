#include <stdio.h>
//problem 6 

int main() {
  int a;
  scanf("%d",&a);
  double x = log2(a);
  if(x==(int) x){
      printf("Yes");
  }
  else if (a == 0){
      printf("Zero is not a valid input");
  }
 else  if(a<0){
      printf("Negative input is not valid");
  }
  else {
      printf("No");
  }
    return 0;
}
