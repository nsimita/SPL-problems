#include <stdio.h>
//problem 4
int main() {
    int n;
    scanf("%d", &n);
    int m[n][n];
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &m[i][j]);

    printf("Major diagonal: ");
    for (int i = 0; i < n; i++)
        printf("%d ", m[i][i]);
   printf("\n");

    printf("Minor diagonal: ");
    for (int i = 0; i < n; i++)
        printf("%d ", m[i][n-1-i]);
     
}
