#include <stdio.h>
int arrSum(int n);
int main() {
  int n;
  scanf("%d",&n);
  int result= arrSum(n);
  printf("Sum In Function: %d",result);
    return 0;
}
int arrSum(int n){
    int arr[n], i, sum=0;
    for(i=0; i<n; i++){
        scanf("%d",&arr[i]);
    }
    for(i=0; i<n; i++){
       sum=sum+arr[i];
    }
    return sum;
}
