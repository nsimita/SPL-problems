#include <stdio.h>
struct student {
    char name [50];
    int id;
    float cgpa;
};

int main()
{
    struct student st;
    printf("Enter name: \n");
    gets(st.name);
    printf("Enter ID: \n");
    scanf("%d", &st.id);
    printf("Enter CGPA: \n");
    scanf("%f", &st.cgpa);
    
    printf("Name: %s\n",st.name);
     printf("ID: %d\n",st.id);
      printf("CGPA: %.2f\n",st.cgpa);
    
    

    return 0;
}
