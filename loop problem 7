#include <stdio.h>
//problem 7
int main(){
    int i=1;
    char g,ch;
     
    while(1) // for inifinite loop 1 is given 
    {
        scanf(" %c",&g );
        if(g !='A')
        {
          printf("Input %d: %c\n",i,g );  
        }
        else
        {
            break;
        }
        i++;
    }

    return 0;
}
